{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Skills",
  "type": "object",
  "properties": {
    "clusters": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "label", "color", "skills"],
        "properties": {
          "id": { "type": "string" },
          "label": { "type": "string" },
          "color": { "type": "string" },
          "skills": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["name", "level"],
              "properties": {
                "name": { "type": "string" },
                "level": { "type": "number", "minimum": 0, "maximum": 100 }
              }
            }
          }
        }
      }
    }
  }
}
